SELECT
  month, 
  new_users,
  ROUND(
  100.0 * (new_users - new_users_last_mo) / new_users_last_mo, 2
  ) || '%' AS monthly_delta
FROM (
  SELECT
    [created_at:month] AS month,
    COUNT(*) AS new_users,
    LAG(count(1), 1) OVER (ORDER BY month) AS new_users_last_mo
  FROM mongodb.identities
  WHERE _type = 'User'
  AND account_verified = 'true'
  GROUP BY 1 
  ORDER BY 1 DESC ) nu
